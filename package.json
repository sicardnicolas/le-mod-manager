  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "electron": "15.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  }
}
